{
  "expo": {
    "scheme": "acme",
    "version": "1.0.0",
    "userInterfaceStyle": "light",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "web": {
      "output": "static",
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Autorizar $(PRODUCT_NAME) a usar o Face ID."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Autorizar $(PRODUCT_NAME) a usar sua localização.",
          "isAndroidBackgroundLocationEnabled": true,
          "isAndroidForegroundServiceEnabled": true
        }
      ],
      [
        "expo-notifications",
        {
          "color": "#ffffff"
        }
      ],
      "expo-router"
    ],
    "name": "aracaju-app",
    "slug": "aracaju-app",
    "android": {
      "package": "com.brunowe.createaupifrontmobile",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyD4yDs6XgPHoXcNXqXnSmj_FQ3DJKcC5H0"
        }
      },
      "permissions": [
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "ios": {
      "bundleIdentifier": "com.brunowe.createaupifrontmobile",
      "infoPlist": {
        "NSFaceIDUsageDescription": "Autorizar $(PRODUCT_NAME) a usar o Face ID."
      }
    },
    "extra": {
      "eas": {
        "projectId": "08c1341a-5fd7-4c3a-87ec-9186924485bd"
      },
      "router": {
        "origin": false
      }
    },
    "owner": "aupi"
  }
}
